<%- model_class = Recoleccion -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:presonas_suben) %>:</strong></dt>
  <dd><%= @recoleccion.presonas_suben %></dd>
  <dt><strong><%= model_class.human_attribute_name(:personas_bajan) %>:</strong></dt>
  <dd><%= @recoleccion.personas_bajan %></dd>
  <dt><strong><%= model_class.human_attribute_name(:long) %>:</strong></dt>
  <dd><%= @recoleccion.long %></dd>
  <dt><strong><%= model_class.human_attribute_name(:lat) %>:</strong></dt>
  <dd><%= @recoleccion.lat %></dd>
  <dt><strong><%= model_class.human_attribute_name(:recorrido) %>:</strong></dt>
  <dd><%= @recoleccion.recorrido %></dd>
  <dt><strong><%= model_class.human_attribute_name(:patente) %>:</strong></dt>
  <dd><%= @recoleccion.patente %></dd>
  <dt><strong><%= model_class.human_attribute_name(:nombre) %>:</strong></dt>
  <dd><%= @recoleccion.nombre %></dd>
  <dt><strong><%= model_class.human_attribute_name(:puerta) %>:</strong></dt>
  <dd><%= @recoleccion.puerta %></dd>
  <dt><strong><%= model_class.human_attribute_name(:periodo) %>:</strong></dt>
  <dd><%= @recoleccion.periodo %></dd>
  <dt><strong><%= model_class.human_attribute_name(:llegada_paradero) %>:</strong></dt>
  <dd><%= @recoleccion.llegada_paradero %></dd>
  <dt><strong><%= model_class.human_attribute_name(:salida_paradero) %>:</strong></dt>
  <dd><%= @recoleccion.salida_paradero %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              recoleccions_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_recoleccion_path(@recoleccion), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              recoleccion_path(@recoleccion),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
